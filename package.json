{
  "name": "Credzy",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "remove-reports": "rimraf ./e2e/features/report/reports",
    "generate-reports": "node ./e2e/features/support/reporter.js",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "husky install",
    "release": "standard-version",
    "commit": "cz",
    "codepush:android": "appcenter codepush release-react -a Credzy/Credzy-1 -d Production",
    "detox_build_release": "detox build --configuration android.emu.release",
    "detox_build_debug": "detox build --configuration android.emu.debug",
    "codepush:ios": "appcenter codepush release-react -a Credzy/Credzy -d Production",
    "cucumber_debug": "node_modules/.bin/cucumber-js ./e2e/features --require-module @babel/register -f json:./e2e/features/report/json/cucumber_report.json --configuration android.emu.debug",
    "cucumber_release": " npm run remove-reports &&  node_modules/cucumber/bin/cucumber-js ./e2e/features --require-module @babel/register -f json:./e2e/features/report/json/cucumber_report.json --configuration android.emu.release && npm run generate-reports"
  },
  "dependencies": {
    "@cucumber/cucumber": "^9.1.2",
    "@gorhom/bottom-sheet": "^4.3.1",
    "@hookform/resolvers": "^2.9.8",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-native-firebase/analytics": "^14.12.0",
    "@react-native-firebase/app": "^14.12.0",
    "@react-native-firebase/crashlytics": "^14.12.0",
    "@react-native-masked-view/masked-view": "^0.2.7",
    "@react-navigation/bottom-tabs": "^6.5.3",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.8.2",
    "@types/react-dom": "^18.0.6",
    "@types/redux-logger": "^3.0.9",
    "appcenter": "4.4.5",
    "appcenter-analytics": "4.4.5",
    "appcenter-crashes": "4.4.5",
    "axios": "^0.27.2",
    "cucumber": "^6.0.7",
    "cucumber-html-reporter": "^5.5.0",
    "detox": "^19.13.0",
    "expo-splash-screen": "^0.17.5",
    "global": "^4.4.0",
    "i18n-js": "^3.9.2",
    "lottie-react-native": "^5.1.4",
    "moment": "^2.29.3",
    "multiple-cucumber-html-reporter": "^1.21.4",
    "react": "18.0.0",
    "react-hook-form": "^7.35.0",
    "react-native": "0.69.3",
    "react-native-biometrics": "^3.0.1",
    "react-native-branch": "^5.5.0",
    "react-native-cli": "^2.0.1",
    "react-native-code-push": "^7.0.4",
    "react-native-date-picker": "^4.2.5",
    "react-native-dotenv": "^3.4.2",
    "react-native-email-link": "^1.14.3",
    "react-native-exit-app": "^1.1.0",
    "react-native-flash-message": "^0.2.1",
    "react-native-flipper": "^0.164.0",
    "react-native-gesture-handler": "2.8.0",
    "react-native-haptic-feedback": "^2.0.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keychain": "^8.1.1",
    "react-native-mask-input": "^1.2.2",
    "react-native-mmkv-storage": "^0.7.6",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^4.12.1",
    "react-native-reanimated": "^2.9.1",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1",
    "react-native-share": "^7.9.0",
    "react-native-shared-element": "^0.8.8",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^12.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-tailwindcss": "^1.1.11",
    "react-native-toast-message": "^2.1.6",
    "react-native-vector-icons": "^9.1.0",
    "react-native-version-info": "^1.1.1",
    "react-native-view-shot": "^3.4.0",
    "react-native-webview": "^11.23.0",
    "react-navigation": "^4.4.4",
    "react-navigation-shared-element": "^3.1.3",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-flipper": "^2.0.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^26.0.23",
    "@types/lodash.memoize": "^4.1.6",
    "@types/react": "^18.0.25",
    "@types/react-native": "^0.66.4",
    "@types/react-native-dotenv": "^0.2.0",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.32.0",
    "husky": "^7.0.4",
    "jest": "^27.0.6",
    "metro": "^0.76.1",
    "metro-react-native-babel-preset": "^0.70.3",
    "promise": "^8.3.0",
    "react-test-renderer": "18.0.0",
    "standard-version": "^9.3.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "18.0.15",
    "@types/react-native": "~0.69.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "tagFormat": "${version}",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version}"
        }
      ]
    ]
  },
  "detox": {
    "configurations": {
      "artifacts": {
        "rootDir": "./artifacts",
        "plugins": {
          "screenshot": {
            "enabled": true,
            "shouldResolvePath": true
          }
        }
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/example.app",
        "build": "export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -project ios/example.xcodeproj -UseNewBuildSystem=NO -scheme example -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -quiet",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11 Pro"
        }
      },
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/example.app",
        "build": "xcodebuild -project ios/example.xcodeproj -UseNewBuildSystem=NO -scheme example -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11 Pro"
        }
      },
      "ios.none": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/example.app",
        "build": "xcodebuild -project ios/example.xcodeproj -UseNewBuildSystem=NO -scheme example -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.none",
        "device": {
          "type": "iPhone 11 Pro"
        },
        "session": {
          "server": "ws://localhost:8099",
          "sessionId": "com.wix.demo.react.native"
        }
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android ; ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug ; cd -",
        "type": "android.emulator",
        "launchApp": "auto",
        "device": {
          "avdName": "Pixel_4_API_30"
        }
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android ; ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release ; cd -",
        "type": "android.emulator",
        "launchApp": "auto",
        "device": {
          "avdName": "Pixel_4_API_30"
        }
      },
      "chrome": {
        "type": "android.apk",
        "binaryPath": "/apps/chrome.apk",
        "packageName": "com.android.chrome"
      }
    }
  }
}